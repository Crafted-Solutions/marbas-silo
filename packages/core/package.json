{
	"name": "@crafted.solutions/marbas-core",
	"version": "0.1.17",
	"description": "Core modules for mabas-databroker access",
	"keywords": [
		"MarBas",
		"DataBrokerAPI",
		"MarBasBuiltIns",
		"MarBasGrainAccessFlag",
		"ajax client"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Crafted-Solutions/marbas-silo.git"
	},
	"author": "hatelamers",
	"license": "MIT",
	"main": "./dist/index.js",
	"type": "module",
	"files": [
		"src/",
		"dist/"
	],
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.cjs"
	},
	"scripts": {
		"build": "rollup ./src/js/index.js --file ./dist/index.cjs --format cjs --external lodash.merge,content-disposition && rollup ./src/js/index.js --file ./dist/index.js --format esm --external lodash.merge,content-disposition",
		"watch": "rollup ./src/js/index.js --file ./dist/index.js --format esm --watch",
		"prepack": "npm run build"
	},
	"dependencies": {
		"content-disposition": "^0.5.4",
		"lodash.merge": "^4.6.2"
	},
	"devDependencies": {
		"rollup": "^4.52.5"
	}
}