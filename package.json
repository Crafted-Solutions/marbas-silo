{
	"name": "@crafted.solutions/marbas-silo",
	"version": "0.1.16",
	"description": "Manages grain silo via marbas-databroker",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/Crafted-Solutions/marbas-silo.git"
	},
	"author": "hatelamers",
	"license": "MIT",
	"workspaces": [
		"./scripts/*",
		"./packages/*"
	],
	"scripts": {
		"build": "webpack --progress",
		"build-dev": "webpack --progress --mode development --env=development",
		"watch": "webpack watch --mode development --env=development",
		"start": "webpack serve --mode development --open",
		"start-jed-debug": "webpack serve --mode development --env=DEBUG_JSONEDITOR --open",
		"po-update-static": "node scripts/po-helpers/po-update-static.js",
		"po-update-dynamic": "node scripts/po-helpers/po-update-dynamic.js",
		"test": "echo \"No test specified yet\" && exit 0",
		"prepack": "npm run build"
	},
	"engines": {
		"node": ">=20"
	},
	"config": {
		"locales": [
			"en",
			"de"
		],
		"defaultLocale": "en"
	},
	"dependencies": {
		"@jbtronics/bs-treeview": "github:hatelamers/bs-treeview",
		"@json-editor/json-editor": "^2.15.2",
		"@popperjs/core": "^2.11.8",
		"bootstrap": "^5.3.7",
		"bootstrap-icons": "^1.13.1",
		"clipboard-copy": "^4.0.1",
		"dompurify": "^3.2.6",
		"jodit": "^4.6.2",
		"jose": "^6.0.12",
		"lodash.merge": "^4.6.2",
		"lodash.startcase": "^4.4.0",
		"oauth4webapi": "^3.6.0",
		"ttag": "^1.8.12"
	},
	"devDependencies": {
		"@types/json-editor": "^0.0.10",
		"autoprefixer": "^10.4.21",
		"concurrently": "^9.2.0",
		"css-loader": "^7.1.2",
		"css-minimizer-webpack-plugin": "^7.0.2",
		"handlebars-loader": "^1.7.3",
		"html-webpack-plugin": "^5.6.3",
		"html-webpack-skip-assets-plugin": "^1.0.4",
		"json-loader": "^0.5.7",
		"mini-css-extract-plugin": "^2.9.2",
		"postcss-loader": "^8.1.1",
		"sass": "^1.89.2",
		"sass-loader": "^16.0.5",
		"style-loader": "^4.0.0",
		"webpack": "^5.100.2",
		"webpack-cli": "^6.0.1",
		"webpack-dev-server": "^5.2.2"
	}
